{
    "author": "Mark Sale",
    "project_name": "Some reasonable(ish) name",

    "algorithm": "GA",

    "GA": {
        "elitist_num": 2,
        "crossoverRate": 0.95,
        "mutationRate": 0.95,
        "sharing_alpha": 0.1,
        "selection": "tournament",
        "selection_size": 2,
        "crossoverOperator": "cxOnePoint",
        "mutate": "flipBit",
        "attribute_mutation_probability": 0.1,
        "niche_penalty": 10
    },

    "remove_run_dir": false,
    "remove_temp_dir": true,

    "random_seed": 11,
    "num_parallel": 4,
    "num_generations": 6,
    "population_size": 4,

    "crash_value": 99999999,

    "penalty": {
        "THETA": 10,
        "OMEGA": 10,
        "SIGMA": 10,
        "convergence": 100,
        "covariance": 100,
        "correlation": 100,
        "conditionNumber": 100,
        "non_influential_tokens": 0.00001
    },

    "downhill_period": 2,
    "num_niches": 2,
    "niche_radius": 2,
    "local_2_bit_search": true,
    "final_downhill_search": true,

    "nmfePath": "c:/nm74g64/util/nmfe74.bat",
    "model_run_timeout": 1200,

    "postprocess": {
        "useR": true,
        "RScriptPath": "C:/Program Files/R/R-4.0.2/bin/Rscript.exe",
        "postRunRCode": "{project_dir}/simplefunc.r",
        "R_timeout": 30,
        "usePython": true,
        "postRunPythonCode": "{project_dir}/../simplefunc_common.py"
    },

    "use_saved_models": false,
    "saved_models_file": "{output_dir}/models0.json",

    "model_run_man": "darwin.RemoteRunManager",

    "grid_manager": {
        "python_path": "~/venv1/Scripts/python",
        "submit_command": "qsub -b y -o {results_dir}/{run_name}.out -e {results_dir}/{run_name}.err -N {job_name}",
        "submit_job_id_re": "Your job (\\w+) \\(\".+?\"\\) has been submitted",
        "poll_command": "qstat -s z",
        "poll_job_id_re": "^\\s+(\\w+)",
        "delete_command": "qdel {project_stem}*"
    }
}
