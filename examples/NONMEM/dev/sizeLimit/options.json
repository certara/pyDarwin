{
    "author": "Certara",
    "algorithm": "GA",

    "GA": {
        "crossover_rate": 0.95,
        "elitist_num": 2,
        "mutation_rate": 0.95,
        "attribute_mutation_probability": 0.1,
        "mutate": "flipBit",
        "niche_penalty": 20,
        "selection": "tournament",
        "selection_size": 2,
        "sharing_alpha": 0.1,
        "crossover_operator": "cxOnePoint"
    },

    "random_seed": 1, 
    "population_size": 20,
    "num_parallel": 4,
    "num_generations": 4,

    "downhill_period": 2,
    "num_niches": 2,
    "niche_radius": 2,
    "local_2_bit_search": true,
    "final_downhill_search": true,

    "crash_value": 99999999,

    "penalty": {
        "theta": 10,
        "omega": 10,
        "sigma": 10,
        "convergence": 100,
        "covariance": 100,
        "correlation": 100,
        "condition_number": 100,
        "non_influential_tokens": 0.00001
    },
    "use_effect_limit":true,
    "effect_limit": 6,

    "remove_run_dir": false,

    "nmfe_path": "c:/nm75g64/util/nmfe75.bat",
    "model_run_timeout": 1200

