{
    "author": "Mark Sale",
    "algorithm": "GA",

    "GA": {
        "crossoverRate": 0.95,
        "elitist_num": 4,
        "mutationRate": 0.95,
        "attribute_mutation_probability": 0.1,
        "mutate": "flipBit",
        "niche_penalty": 20,
        "selection": "tournament",
        "selection_size": 2,
        "sharing_alpha": 0.1,
        "crossoverOperator": "cxOnePoint"
