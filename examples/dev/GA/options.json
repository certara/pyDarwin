{
	"author": "Mark Sale",
	"algorithm":"GA",
	"random_seed": 11,
	"population_size": 4,
	"max_model_list_size": 500,
	"num_parallel": 4,
	"num_generations": 6,

	"crash_value": 99999999,

	"remove_run_dir": false,

	"THETAPenalty": 10,
	"OMEGAPenalty": 10,
	"SIGMAPenalty": 10,
	"covergencePenalty": 100,
	"covariancePenalty": 100,
	"correlationPenalty": 100,
    "correlationLimit": 0.95,
	"conditionNumberPenalty": 100,
	"non_influential_tokens_penalty": 0.00001,

	"downhill_period": 2,

	"local_2_bit_search": true,
	"final_downhill_search": true,

	"num_niches": 2,
	"niche_radius": 2,

	"elitist_num": 2,
	"crossoverRate": 0.95,
	"mutationRate": 0.95,
	"sharing_alpha": 0.1,
	"selection": "tournament",
	"selection_size": 2,
	"crossoverOperator": "cxOnePoint",
	"mutate": "flipBit",
	"attribute_mutation_probability": 0.1,
	"niche_penalty": 10,

	"nmfePath": "c:/nm74g64/util/nmfe74.bat",
	"NM_priority_class": "below_normal",
    "NM_timeout_sec": 1200,
    "R_timeout_sec": 30,

	"useR": true,
	"RScriptPath": "C:/Program Files/R/R-4.0.2/bin/Rscript.exe",
	"postRunRCode": "{project_dir}/simplefunc.r",
	"usePython": true,
	"postRunPythonCode": "{project_dir}/../simplefunc_common.py",

	"use_prev_models": false,
	"prev_model_list": "{output_dir}/models0.json",


	"grid_manager": {
		"python_path": "~/venv1/Scripts/python",
		"submit_command": "qsub -b y",
		"submit_job_name_arg": "-N",
		"submit_job_id_re": "Your job (\\w+) \\(\".+?\"\\) has been submitted",
		"poll_command": "qstat -s z",
		"poll_job_id_re": "^\\s+(\\w+)",
		"delete_command": "qdel"
	}
}
