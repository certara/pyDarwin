    },

    "remove_run_dir": false,

    "nmfePath": "c:/nm74g64/util/nmfe74.bat",
    "model_run_timeout": 1200,

    "postprocess": {
        "useR": true,
        "postRunRCode": "{project_dir}/Cmaxppc.r",
        "RScriptPath": "c:\\Program Files\\R\\R-4.1.3\\bin\\Rscript.exe",
        "R_timeout": 120,
        "usePython": false
    }
