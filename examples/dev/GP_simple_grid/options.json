{
	"author": "Mark Sale",
	"algorithm":"GP",
	"random_seed": 11, 
	"population_size": 10,
	"num_opt_chains": 2,
	"nmfePath": "c:/nm74g64/util/nmfe74.bat",
	"num_parallel": 4,
	"num_generations": 6,
	"num_niches": 2,
	"niche_penalty": 10, 
	"niche_radius": 2,
	"crash_value": 99999999, 
	"crossoverRate": 0.95, 
	"downhill_period":2,
	"elitist_num": 4,
	"mutationRate": 0.95, 
	"attribute_mutation_probability": 0.1, 
	"mutate": "flipBit",
	"local_2_bit_search": false,
	"final_downhill_search": true, 
	"selection": "tournament",
	"selection_size": 2,  
	"sharing_alpha": 0.1,  

    "nmfePath": "/opt/nm751/util/nmfe75",
    "model_run_timeout": 1200,
    "R_timeout": 30,

    "useR": false,
    "usePython": false,

    "model_run_man": "darwin.RemoteRunManager",

    "remove_run_dir": true,
    "temp_dir": "{project_dir}/temp",

    "grid_manager": {
            "python_path": "~/darwin/venv/bin/python",
            "submit_command": "qsub -b y",
            "submit_job_name_arg": "-N",
            "submit_job_id_re": "Your job (\\w+) \\(\".+?\"\\) has been submitted",
            "poll_command": "qstat -s z",
            "poll_job_id_re": "^\\s+(\\w+)",
            "delete_command": "qdel"
    },

	"use_saved_models": true,
	"saved_models_file": "{project_dir}/models0.json"
}
