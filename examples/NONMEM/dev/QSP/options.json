{
    "author": "Certara",
    "algorithm": "MOGA3",
    "num_opt_chains": 4,
    "skip_running": true,
    "crash_value": 99999999,
     "MOGA": {
        "objectives": 3,
        "constraints": 1,
        "partitions":4,
        "crossover_rate": 0.95,
        "mutation_rate": 0.95
      },

    "random_seed": 1,
    "population_size": 40,
    "num_parallel": 4,
    "num_generations": 6,

    "downhill_period": 99,
    "num_niches": 2,
    "niche_radius": 2,
    "final_downhill_search": false,

    "rerun_front_models": false,

    "penalty": {
        "theta": 0,
        "omega": 0,
        "sigma": 0,
        "convergence": 0,
        "covariance": 0,
        "correlation": 0,
        "condition_number": 0,
        "non_influential_tokens": 0
    },
    "remove_run_dir": false,
    "model_run_timeout": 1200,

    "postprocess": {
        "use_python": true,
        "post_run_python_code": "{project_dir}/RunQSP.py"
    }
}
